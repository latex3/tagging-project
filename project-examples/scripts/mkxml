 for i in *.pdf
 do
 echo $i
 texlua ../../../pdf_structure/show-pdf-tags/show-pdf-tags.lua --xml  "$i" > "${i/.pdf/.xml}" 2>"${i/.pdf/xml.log}"
 done

for i in *.pdf
 do
 echo "--map $i"
 texlua ../../../pdf_structure/show-pdf-tags/show-pdf-tags.lua --xml  --map "$i" > "${i/.pdf/map.xml}" 2> "${i/.pdf/mapxml.log}"
done